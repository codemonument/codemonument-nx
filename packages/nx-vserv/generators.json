{
  "$schema": "http://json-schema.org/schema",
  "name": "nx-vserv",
  "version": "0.0.1",
  "generators": {
    "app": {
      "factory": "./src/generators/app/generator",
      "schema": "./src/generators/app/schema.json",
      "description": "nx-vserv app generator"
    },
    "ansible-runner": {
      "factory": "./src/generators/ansible-runner/generator",
      "schema": "./src/generators/ansible-runner/schema.json",
      "description": "Generates an ansible runner app for running your vserv apps"
    }
  }
}
